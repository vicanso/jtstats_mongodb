{
  "name" : "jtstats-mongodb",
  "instances" : 1,
  "merge_logs" : true,
  "max-memory-restart" : "100M",
  "script" : "app.js",
  "log_file" : "/vicanso/pm2/log/jtstats-mongodb.log",
  "error_file" : "/vicanso/pm2/log/jtstats-mongodb-error.log",
